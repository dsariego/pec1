{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "Repositorio trabajos asignatura Herramientas HTML y CSS",
  "main": "babel.config.js",
  "scripts": {
    "server": "live-server uoc --open",
    "parcel:dev": "parcel uoc/index.html",
    "parcel:build": "parcel build uoc/index.html",
    "dev": "npm-run-all clean parcel:dev",
    "build": "npm-run-all clean parcel:build",
    "clean": "rimraf dist .cache .cache-loader",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" --quiet",
    "test": "jest --passWithNoTests --silent"
  },
  "keywords": [
    "UOC",
    "HTML",
    "CSS",
    "Typescript",
    "Node js",
    "npm",
    "instancias",
    "github"
  ],
  "author": {
    "name": "Daniel Sariego Alvarez",
    "email": "danielsariego82@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/types": "^7.13.14",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^23.6.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "live-server": "^1.2.1",
    "minecraft-data": "^2.83.1",
    "npm-run-all": "^4.1.5"
  }
}
